{
	"$id": "http://localhost:5173/schemas/small/v1/lifecycle.schema.json",
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "array",
	"items": {
		"type": "object",
		"required": ["type", "at", "replayId"],
		"properties": {
			"type": {
				"type": "string",
				"description": "Lifecycle event type (e.g., 'validated', 'materialized')"
			},
			"at": {
				"type": "string",
				"format": "date-time",
				"description": "ISO 8601 timestamp when the event occurred"
			},
			"replayId": {
				"type": "string",
				"description": "Deterministic replay identifier computed from protocol version and manifest"
			}
		}
	}
}
